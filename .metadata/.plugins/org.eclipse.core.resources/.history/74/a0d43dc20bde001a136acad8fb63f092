package JavaPrograms;

public class Practise {

	public static void main(String[] args) {
		
		isprime(17);
		
	}
	
	public static boolean isprime(int num) {
		
		if (num <=1) {
			
			return false;
		} 
		
		for (int i = 2; i < num; i++) {
			if (num % i== 0) {
				return false;
			}
		}
		
		return true;
	}

}
