package JavaPrograms;

public class Practise {

	public static void main(String[] args) {
		
		int a[] = {10,20,30,40,50,900};
		
		int largest = a[0];
		int smallest = a[0];
		
		for (int i = 1; i < a.length; i++) {
			if (a[i]>largest) {
				System.out.println("Largest"+ a[i]);
			}
			else(a[i]<smallest) {
				System.out.println("smallest"+ a[i]);
			}
		}
		
	}
}
