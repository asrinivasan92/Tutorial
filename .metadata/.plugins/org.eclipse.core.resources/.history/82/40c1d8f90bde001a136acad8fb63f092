package JavaPrograms;

public class Practise {

	public static void main(String[] args) {
		
		System.out.println(isfact(5));
		
	}
	
	public static boolean isfact(int num) {
		
		if (num <=1) {
			
			return false;
		} 
		
		for (int i = 2; i < num; i++) {
			if (num % i== 0) {
				return false;
			}
		}
		
		return true;
	}

}
